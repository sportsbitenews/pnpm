#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm/5.4.2/node_modules/npm/node_modules/mkdirp/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm/5.4.2/node_modules/npm/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm/5.4.2/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm/5.4.2/node_modules/npm/node_modules/mkdirp/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm/5.4.2/node_modules/npm/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm/5.4.2/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules" node  "$basedir/../mkdirp/bin/cmd.js" "$@"
  ret=$?
fi
exit $ret
