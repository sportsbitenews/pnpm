#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/conventional-commits-parser/1.3.0/node_modules/conventional-commits-parser/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/conventional-commits-parser/1.3.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/conventional-commits-parser/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@commitlint/core/3.2.0/node_modules/conventional-commits-parser/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@commitlint/core/3.2.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@commitlint/core/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@commitlint/node_modules" "$basedir/node"  "$basedir/../../../../conventional-commits-parser/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/conventional-commits-parser/1.3.0/node_modules/conventional-commits-parser/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/conventional-commits-parser/1.3.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/conventional-commits-parser/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@commitlint/core/3.2.0/node_modules/conventional-commits-parser/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@commitlint/core/3.2.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@commitlint/core/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@commitlint/node_modules" node  "$basedir/../../../../conventional-commits-parser/cli.js" "$@"
  ret=$?
fi
exit $ret
