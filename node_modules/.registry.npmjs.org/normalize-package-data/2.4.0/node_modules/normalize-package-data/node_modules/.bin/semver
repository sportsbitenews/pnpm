#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/semver/5.4.1/node_modules/semver/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/semver/5.4.1/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/semver/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/normalize-package-data/2.4.0/node_modules/semver/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/normalize-package-data/2.4.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/normalize-package-data/node_modules" "$basedir/node"  "$basedir/../../../semver/bin/semver" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/semver/5.4.1/node_modules/semver/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/semver/5.4.1/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/semver/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/normalize-package-data/2.4.0/node_modules/semver/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/normalize-package-data/2.4.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/normalize-package-data/node_modules" node  "$basedir/../../../semver/bin/semver" "$@"
  ret=$?
fi
exit $ret
