#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/json2yaml/1.1.0/node_modules/json2yaml/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/json2yaml/1.1.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/json2yaml/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/pnpm-file-reporter/0.0.1/node_modules/json2yaml/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/pnpm-file-reporter/0.0.1/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/pnpm-file-reporter/node_modules" "$basedir/node"  "$basedir/../../../json2yaml/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/json2yaml/1.1.0/node_modules/json2yaml/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/json2yaml/1.1.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/json2yaml/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/pnpm-file-reporter/0.0.1/node_modules/json2yaml/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/pnpm-file-reporter/0.0.1/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/pnpm-file-reporter/node_modules" node  "$basedir/../../../json2yaml/cli.js" "$@"
  ret=$?
fi
exit $ret
