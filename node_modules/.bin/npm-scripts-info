#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm-scripts-info/0.3.6/node_modules/npm-scripts-info/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm-scripts-info/0.3.6/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm-scripts-info/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/npm-scripts-info/node_modules" "$basedir/node"  "$basedir/../npm-scripts-info/lib/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm-scripts-info/0.3.6/node_modules/npm-scripts-info/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm-scripts-info/0.3.6/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/npm-scripts-info/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/npm-scripts-info/node_modules" node  "$basedir/../npm-scripts-info/lib/cli.js" "$@"
  ret=$?
fi
exit $ret
