#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/.registry.npmjs.org/commitizen/2.9.6/node_modules/commitizen/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/.registry.npmjs.org/commitizen/2.9.6/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/.registry.npmjs.org/commitizen/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/commitizen/node_modules" "$basedir/node"  "$basedir/../../../pnpm-default-reporter/node_modules/commitizen/bin/git-cz" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/.registry.npmjs.org/commitizen/2.9.6/node_modules/commitizen/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/.registry.npmjs.org/commitizen/2.9.6/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/.registry.npmjs.org/commitizen/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm-default-reporter/node_modules/commitizen/node_modules" node  "$basedir/../../../pnpm-default-reporter/node_modules/commitizen/bin/git-cz" "$@"
  ret=$?
fi
exit $ret
